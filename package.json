{
  "author": "Dylan Archer <dylarcher@gmail.com> (@dylarcher)",
  "bin": {
    "git.starter-template": "./bin/cli.js"
  },
  "browser": "dist/index.umd.js",
  "bugs": {
    "url": "https://github.com/dylarcher/git.starter-template/issues"
  },
  "description": "A github starter template for creating a new repository with a predefined structures, documents and configurations.",
  "dependencies": {
    "yargs": "18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "22.15.30",
    "@types/yargs": "17.0.33",
    "markdownlint-cli": "0.45.0",
    "yaml-lint": "1.7.0"
  },
  "directories": {
    ".github": ".github",
    "dist": "dist",
    "docs": "docs",
    "node_modules": "node_modules",
    "src": "src",
    "test": "test"
  },
  "engines": {
    "node": ">=20.19.2",
    "npm": ">=10.8.2",
    "vscode": "1.100.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://dylarcher.github.io/git.starter-template",
  "keywords": [
    "state",
    "management",
    "cli",
    "library",
    "npm",
    "javascript"
  ],
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "name": "enter-repository-namespace",
  "optionalDependencies": {
    "@types/chrome": "0.0.326",
    "@types/vscode": "1.100.0",
    "c8": "10.1.3",
    "glob": "11.0.2",
    "rimraf": "6.0.1"
  },
  "packageManager": "npm@10.8.2",
  "peerDependencies": {
    "markdownlint": "0.38.0",
    "typescript": "5.8.3",
    "yargs-parser": "22.0.0"
  },
  "private": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dylarcher/git.starter-template.git"
  },
  "scripts": {
    "build": "tsc --declarationMap false --emitDeclarationOnly false && npm run bundle:types",
    "bundle:types": "dts-bundle-generator -o dist/types.d.ts src/**/*.ts",
    "clean": "npm run clean:dist && npm run clean:libs",
    "clean:dist": "rimraf dist lib test/coverage .vscode-test types",
    "clean:libs": "rimraf node_modules package-lock.json .cache/npm",
    "format": "biome format --write --config-path .github/configs/biome.json src && markdownlint --config .github/configs/.markdownlintrc.json --fix .github/**/*.md docs/**/*.md src/**/*.md README.md CHANGELOG.md",
    "lint": "biome lint --write --config-path .github/configs/biome.json src && yamllint -c .github/configs/yaml-lint.yaml .github/**/*.yml",
    "test": "c8 --config .github/configs/.c8rc.json node --test"
  },
  "type": "module",
  "version": "0.1.0"
}
